<Window x:Class="Common.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="46,32,0,0" TextWrapping="Wrap" Text="{Binding A}" VerticalAlignment="Top" Width="120" Name="A"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="270,32,0,0" TextWrapping="Wrap" Text="{Binding B}" VerticalAlignment="Top" Width="120" Name="B"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="177,81,0,0" VerticalAlignment="Top" Width="75" Command="{Binding CalculateCommand}"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="163,147,0,0" TextWrapping="Wrap" Text="{Binding R}" VerticalAlignment="Top" Width="120" Name="R"/>

    </Grid>
</Window>
